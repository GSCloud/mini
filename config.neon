### TESSERACT MINI

# uncomment to force debug !!!
#dbg: TRUE

# CORE
app: mini
canonical_url: "https://mini.gscloud.cz/"
date_default_timezone: "Europe/Prague"
project: TESSERACT
secret_cookie_key: secure.key
version: v1

# GCP ID
gcp_project_id: "gscloudcz-163314"

# Google OAUTH 2.0
goauth_origin: "https://mini.gscloud.cz"
goauth_redirect: "https://mini.gscloud.cz/login"
local_goauth_origin: "http://localhost:8020"
local_goauth_redirect: "http://localhost:8020/login"

# ALPHA/BETA
alpha_hosts: [
  mini
  amini.gscloud.cz
]
beta_hosts: [
  mini
  bmini.gscloud.cz
]
multisite_profiles: [
  default: [
    mini
    amini.gscloud.cz
    bmini.gscloud.cz
  ]
]

